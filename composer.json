{
    "name": "made-foryou/filament-pages",
    "description": "Package for managing pages within Filament.",
    "license": "MIT",
    "version": "0.1.1",
    "autoload": {
        "psr-4": {
            "MadeForYou\\FilamentPages\\": "src/",
            "MadeForYou\\FilamentPages\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MadeForYou\\FilamentPages\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Menno Tempelaar",
            "email": "menno@made-foryou.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.3",
        "spatie/laravel-package-tools": "^1.16",
        "filament/filament": "^3.2",
        "made-foryou/routes": "^0.2",
        "made-foryou/filament-helpers": "^0.1"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^8.21"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadeForYou\\FilamentPages\\FilamentPagesServiceProvider"
            ]
        }
    }
}
